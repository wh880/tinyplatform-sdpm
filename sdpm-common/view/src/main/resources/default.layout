#set(pagePostionTitle='<li><a href="'+TINY_CONTEXT_PATH+'">首页</a></li>',activeMenuId="1",topMenuId="0",c=call("getMenuId"),c=call("sdpmgGtMenuId"))
<!DOCTYPE html>
