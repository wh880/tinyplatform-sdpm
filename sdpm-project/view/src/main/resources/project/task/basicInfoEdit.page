#@div(divClass="infoContent")
<p class="titleName">工时信息</p>
<table class="tableInfoes">
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">预计开始</td>
        <td style="width:75%">
            #@formControlDiv("col-8")
			     	#@tinyDateInput(
                "taskEstStared" task?.taskEstStared "YYYY-MM-DD" {"style":"width:140px","datatype":"*","placeholder":"选择日期"})#end
      			#end
        </td>
    </tr>
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">实际开始</td>
        <td style="width:75%">
            #@formControlDiv("col-8")
			     	#@tinyDateInput(
                "taskRealStarted" task?.taskRealStarted "YYYY-MM-DD" {"style":"width:140px","datatype":"*","placeholder":"选择日期"})#end
      			#end
        </td>
    </tr>
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">截止日期</td>
        <td style="width:75%">
            #@formControlDiv("col-8")
			     	#@tinyDateInput(
                "taskDeadLine" task?.taskDeadLine "YYYY-MM-DD" {"style":"width:140px","datatype":"*","placeholder":"选择日期"})#end
      			#end
        </td>
    </tr>
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">最初预计</td>
        <td style="width:54%">
            <input name="taskEstimate" type="text" value="${task?.taskEstimate}" style="height:33px"/>
        </td>
    </tr>
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">总耗时</td>
        <td>${task?.Float}
            #@simpleModal("myModal" "新增日志")#end
            #@iconOperations("#myModal" "icon-time" "新增日志" "900" "400"
                "/project/task/consumeTime.pagelet" "ml-5")#end
        </td>
    </tr>
    <tr>
        <td style="float:right;padding:3px 10px;font-size:15px">预计剩余</td>
        <td style="width:75%">
            <input name="taskLeft"type="text" value="${task?.taskLeft}" style="height:33px"/>
        </td>
    </tr>
</table>
#end
