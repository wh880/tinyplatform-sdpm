


<script >
	
	$(document).ready(function(){
	
		$(".secondMenu").each(function(){
			
			$(this).on("click",function(){
				var inputname = $(this).attr("tabindex");
				var inputvalue = $(this).html().trim();
				var i = 0;
				$("input[type='checkbox']").each(function(){
					
					if(this.checked==true){
						var hm = "<input type='hidden' name='" +inputname + "[" + i +"]' value='" + inputvalue + "'/>";
						this.name = this.name +ã€€"[" + i +"]";
						
						$(this).parent().append(hm);
			  		
			  			++i;
					}
				
				});
				
				$("form[name='storyform']").trigger("submit");
				
				
				
			});
			
		});
	
	});
	
</script>

