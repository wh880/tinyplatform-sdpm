<script>
    $(document).ready(function () {

        $("#story_reviewed").on("click", function () {

            var vm = this.checked;
            if (vm == true) {
                $("select[name='storyReviewedBy']").attr("disabled", "disabled");
            }
            if (vm == false) {
                $("select[name='storyReviewedBy']").removeAttr("disabled");
            }


        });

    });
</script>





