<script>
    $(document).ready(function () {
        $("select[name='storySource']").bind('change', function () {
            if (0 == $(this).val()) {
                $(".customer-request").show();
                $("#demand-sources").addClass("col-5");
            } else {
                $(".customer-request").hide();
                $("#demand-sources").removeClass("col-5");
            }
        });

    });

</script>
